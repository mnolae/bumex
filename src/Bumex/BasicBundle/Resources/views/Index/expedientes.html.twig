{% extends "BumexBasicBundle::layout.html.twig" %}

{% block title "Expedientes" %}

{% block content_header_more %}
	<li><a href="{{ path('index') }}">Inicio</a></li>
	<li><a href="{{ path('historial') }}">Historial</a></li>
{% endblock %}

{% block cabecera %} 
	<h2>Informaci√≥n de expedientes revisados</h2>
{% endblock %}

{% block content %}

	<table>
	<thead>
	    <tr>
	        <th scope="col">Dato</th>
	        <th scope="col">Valor</th>
	    </tr>
	</thead>
	<tbody>
	    {% for key, dato in datos %}
	        <tr>
	            <th>{{ key }}</th>
	            <td>{{ dato }}</td>
	        </tr>
	    {% endfor %}
	</tbody>
	</table>
    
{% endblock %}
