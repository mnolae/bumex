{% extends "BumexBasicBundle::layout.html.twig" %}

{% block title "Expedientes" %}

{% block content_header_more %}
	<li><a href="{{ path('index') }}">Inicio</a></li>
	<li><a href="{{ path('historial') }}">Historial</a></li>
{% endblock %}

{% block cabecera %} 
	<h2>Información de expedientes revisados</h2>
{% endblock %}

{% block content %}

	<table>
	<thead>
	    <tr>
	        <th scope="col">Dato</th>
	        <th scope="col">Valor</th>
	    </tr>
	</thead>
	<tbody>
        <tr>
            <th>Fecha de búsqueda</th>
            <td>{{ datos.fecha }}</td>
        </tr>
		<tr>
            <th>Número de edictos</th>
            <td>{{ datos.nedictos }}</td>
        </tr>
        <tr>
            <th>Número de expedientes</th>
            <td>{{ datos.nexpedientes }}</td>
        </tr>
        <tr>
            <th>Número de coincidencias</th>
            <td>{{ datos.ncoincidencias }}</td>
        </tr>
        <tr>
            <th>Número teléfonos encontrados</th>
            <td>{{ datos.fecha }}</td>
        </tr>
	</tbody>
	</table>
    
{% endblock %}
